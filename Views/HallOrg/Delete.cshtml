@model MvcApplication4.Models.SubHall

@{
    Layout = "~/Views/Shared/_cmstLayout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete</title>
    <script src="~/Content/bootstrap.js"></script>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.7.1.js"></script>
</head>
<body>
    <div class="row">
        <div class="col-md-3"></div>
        <div class="col-md-6">
            <h2>Are you Sure you want to Delete this Hall</h2>
            <img src="@Url.Content(Model.Image)" style="width:150px;height:150px" /><br />
            <span style="font-weight:800">Name: </span> @Model.Name <br />
            <span style="font-weight:800">Capacity: </span> @Model.Capacity <span>Peoples</span> <br />
            <span style="font-weight:800">Address: </span> @Model.Address <br />
            <span style="font-weight:800">Phone: </span> @Model.Phone <br /><br />
            <a href="~/HallOrg/DeleteConfirm?id=@Model.Id"><input type="button" value="Delete" /></a>
            <a href="~/HallOrg/OrganizerData?orgid=@Model.OrgId"><input type="button" value="Cancel" /></a>
        </div>
        <div class="col-xs-3"></div>
    </div>
</body>
</html>
